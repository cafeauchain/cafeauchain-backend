<% content_for :body_class, current_roaster ? "customer" : "" %>
<% content_for :title, "Register" %>
<%= react_component "main", props = {
    url: password_path(resource_name),
    header_info: current_roaster ? {url: current_roaster.logo_image_url, name: current_roaster.name} : nil,
    extra: current_user,
    token: params[:reset_password_token],
    fieldType: "reset_password",
    component: "login"
} %>


